package ex2;

import java.util.Date;

public abstract class ClockTime {

	protected int hour;
	protected int minute;
	protected int second;
	
	public ClockTime(int hour, int minute, int second) {
		super();
		Date date = new Date();
		this.hour = date.getHours();
		this.minute = minute;
		this.second = second;
	}

}
